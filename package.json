{
  "name": "tools-page",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "test": "npm run lint && ng test --watch=false",
    "test:w": "npm run lint && ng test",
    "cover": "ng test --watch=false --code-coverage",
    "lint": "tslint src/**/*.ts",
    "local": "npm run prebuild && ng serve --environment=local",
    "dev": "npm run prebuild && ng serve --environment=dev --host 0.0.0.0",
    "stage": "npm run prebuild && ng serve --environment=stage --host 0.0.0.0",
    "prod": "npm run prebuild && ng serve --environment=prod --host 0.0.0.0",
    "standalone-ws": "npm run prebuild && ng serve --environment=docker --host 0.0.0.0",
    "prebuild": "./copy_css.sh",
    "build:stage": "npm run prebuild && ng build --output-hashing bundles --base-href /tools/ --target=production --environment=stage",
    "build:dev": "npm run prebuild && ng build --output-hashing bundles --base-href /tools/ --target=development --environment=dev",
    "build:prod": "npm run prebuild && ng build --output-hashing bundles --base-href /tools/ --target=production --environment=prod",
    "build:demo": "npm run prebuild && ng build --output-hashing bundles --base-href /ng2-tools-page/ --target=development --environment=dev",
    "build:docker": "npm run prebuild && ng build --output-hashing bundles --base-href /tools/ --target=production --environment=docker",
    "start": "npm run dev",
    "stop:blue": "forever stopall",
    "start:blue": "npm run build:prod && mkdir -p logs && forever start -l forever.log -o logs/out.log -e logs/error.log -a -p logs --minUptime 1000 --spinSleepTime 1000 ./node_modules/.bin/ws -d dist -p 3001 --rewrite '/assets/fonts/* -> /tools/assets/fonts/$1' '/assets/images/* -> /tools/assets/images/$1' '/tools/bubbles -> /tools/' '/tools/map -> /tools/' '/tools/mountain -> /tools/'",
    "start:dev": "npm run build:dev && npm run start:local-web-server",
    "start:local-web-server": "ws -d ./dist/ -p 3001 --rewrite '/assets/fonts/* -> /tools/assets/fonts/$1' '/assets/images/* -> /tools/assets/images/$1' '/tools/bubbles -> /tools/' '/tools/map -> /tools/' '/tools/mountain -> /tools/'",
    "e2e:local": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/.bin/protractor protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@angular/router": "4.1.3",
    "@types/lodash": "4.14.65",
    "angulartics2": "2.2.1",
    "core-js": "2.4.1",
    "d3": "4.9.1",
    "forever": "0.15.3",
    "lodash": "4.17.4",
    "ng2-vizabi": "1.1.1",
    "rxjs": "5.4.0",
    "ts-helpers": "1.1.2",
    "urlon": "2.0.1",
    "vizabi": "0.23.0-38",
    "vizabi-barrankchart": "1.0.20-0",
    "vizabi-bubblechart": "1.0.19-0",
    "vizabi-bubblemap": "1.0.12-1",
    "vizabi-config-systema_globalis": "1.2.6",
    "vizabi-linechart": "1.0.16-0",
    "vizabi-mountainchart": "1.0.16-2",
    "vizabi-ws-reader": "0.2.0",
    "vizabi-popbyage": "1.2.2-2",
    "zone.js": "0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "1.0.6",
    "@types/jasmine": "2.5.51",
    "@types/node": "^7.0.31",
    "codelyzer": "3.0.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.31",
    "local-web-server": "1.2.7",
    "protractor": "5.1.2",
    "ts-node": "3.0.6",
    "tslint": "5.4.3",
    "tslint-config-valorsoft": "1.2.0",
    "typescript": "2.3.4",
    "webdriver-manager": "12.0.6"
  }
}
