{
  "name": "tools-page",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "test": "npm run lint && ng test --watch=false",
    "test:w": "npm run lint && ng test",
    "cover": "ng test --watch=false --code-coverage",
    "lint": "tslint src/**/*.ts",
    "local": "npm run prebuild && ng serve --environment=local",
    "dev": "npm run prebuild && ng serve --environment=dev",
    "stage": "npm run prebuild && ng serve --environment=stage",
    "prod": "npm run prebuild && ng serve --environment=prod",
    "prebuild": "cp ./node_modules/vizabi/build/dist/vizabi.css ./src/assets/vizabi.css",
    "build:stage": "npm run prebuild && ng build --base-href /tools/ --target=production --environment=stage",
    "build:dev": "npm run prebuild && ng build --base-href /tools/ --target=development --environment=dev",
    "build:prod": "npm run prebuild && ng build --base-href /tools/ --target=production --environment=prod",
    "build:demo": "npm run prebuild && ng build --base-href /ng2-tools-page/ --target=development --environment=dev",
    "build:docker": "npm run prebuild && ng build --base-href /tools/ --target=production --environment=docker",
    "start": "npm run dev",
    "start:dev": "npm run build:dev && ws -d ./dist/ -p 3011 --rewrite '/tools/bubbles -> /tools/' '/tools/map -> /tools/' '/tools/mountain -> /tools/'"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/compiler-cli": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/platform-server": "2.4.8",
    "@angular/router": "3.4.6",
    "@types/lodash": "4.14.39",
    "URLON": "2.0.0",
    "angulartics2": "1.6.3",
    "core-js": "2.4.1",
    "d3": "4.5.0",
    "lodash": "4.17.2",
    "ng2-vizabi": "~0.19.2",
    "rxjs": "5.0.1",
    "ts-helpers": "1.1.2",
    "vizabi": "0.19.1",
    "vizabi-config-systema_globalis": "^1.0.0",
    "vizabi-ws-reader": "0.1.2",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32",
    "@types/jasmine": "2.2.30",
    "@types/node": "6.0.42",
    "codelyzer": "2.0.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "local-web-server": "1.2.6",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "4.4.2",
    "tslint-config-valorsoft": "1.2.0",
    "typescript": "2.0.10",
    "webdriver-manager": "10.2.5"
  }
}
